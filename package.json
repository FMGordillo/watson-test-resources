{
  "name": "watson-test-resources",
  "version": "2.0.0",
  "description": "web application that hosts resources to run integration tests for the watson sdks",
  "repository": {
    "type": "git",
    "url": "https://github.com/germanattanasio/watson-test-resources.git"
  },
