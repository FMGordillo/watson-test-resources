    "helmet": "^1.3.0"
  },
  "devDependencies": {
    "supertest": "^1.2.0",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5"
  }
}